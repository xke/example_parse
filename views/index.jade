doctype 5
html
	head
		meta(charset="utf-8")
		title Test
		link(rel="stylesheet", href="css/bootstrap.min.css")
		script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.6/angular.min.js")
	body
		.container
			.row
				.col-md-12
					table.table.table-hover
						tr
							th Place
							th Surname
							th Name
							th CreatedAt
							th updatedAt 
						for item in data
							tr
								td= item.place
								td= item.surname
								td= item.name
								td= item.createdAt
								td= item.updatedAt